
model UserRoles {
  userId     String
  roleId     String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now()) @updatedAt
  deletedAt  DateTime?

  user User @relation(fields: [userId], references: [id])
  role Role @relation(fields: [roleId], references: [id])

  @@id([userId, roleId])
  @@map("user_roles")
}